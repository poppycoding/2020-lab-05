define("question-new:widget/ask/pc-exp/ask-img",function(i){var e=i("common:widget/lib/jquery/jquery"),n=i("question-new:widget/read-opt/img-preview/preview"),o=e(".q-img-ul"),t=e(".q-img-li"),l=e(".q-img-con"),m=o.width(),p=l.width();p>=m&&(e(".q-img-prev-wp").remove(),e(".q-img-next-wp").remove()),e(".q-img-next").on("click",function(){var i=o.position().left;o.animate(m+i>=2*p?{left:"-"+(p-i)+"px"}:{left:"-"+(m-p)+"px"})}),e(".q-img-prev").on("click",function(){var i=o.position().left;o.animate(-p>=i?{left:i+p+"px"}:{left:"0px"})}),t.on("click",function(){var i=e(this).find(".q-img-item").clone(),n=e('<div class="q-img-fullscreen-wraper"></div>');n.append(i),e("body").append(n).addClass("q-img-body-noscroll"),i.css({marginLeft:-i.width()/2+"px",marginTop:-i.height()/2+"px"}),n.click(function(i){i.stopPropagation(),e("body").removeClass("q-img-body-noscroll"),e(this).remove()})}),e(".replyask-box .ikqb_img_alink").click=null,new n({$el:".replyask-box .ikqb_img_alink",imgAttr:"href"})});