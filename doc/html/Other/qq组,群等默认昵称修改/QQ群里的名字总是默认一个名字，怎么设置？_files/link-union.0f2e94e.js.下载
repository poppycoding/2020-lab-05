define("question-new:widget/link-union/link-union",function(i,n,d){var o=i("common:widget/js/util/tangram/tangram");d.exports={init:function(i){var n=this,d=i.newbest||[],t=i.recommend||[],e=i.answerList||[],h=i.askQid||0;window.baidu_hh_hotword={id:[],page_url:"http://zhidao.baidu.com/question/"+h,di:"u2146910",maxword:10,frequency:3,sid:i.sid,isMobile:!1},o.each(d,function(i,d){window.baidu_hh_hotword.id.push("best-content-"+d.id),n.addNoHighlightClass("#best-content-"+d.id)}),o.each(t,function(i,d){window.baidu_hh_hotword.id.push("recommend-content-"+d.id),n.addNoHighlightClass("#recommend-content-"+d.id)}),o.each(e,function(i,d){window.baidu_hh_hotword.id.push("answer-content-"+d.id),n.addNoHighlightClass("#answer-content-"+d.id)})},addNoHighlightClass:function(i){o(i).find('pre[t="code"]').each(function(i,n){o(n).addClass("baidu-gh-highlight-no")})}}});