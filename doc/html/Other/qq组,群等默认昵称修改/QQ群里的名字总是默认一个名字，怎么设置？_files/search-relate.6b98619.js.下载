define("question-new:widget/ask/search-relate/search-relate",function(e,t,a){function i(){n.addKey({searchRelateNew:1,isQ:!!window.isQualityView}),s(".wgt-search-relate .word").on("click",function(){if(s(this).hasClass("word-search")){n.send({page:"question",area:"search-relate-word-search",action:"click",type:2014,isQ:!!window.isQualityView});var e=s(".wgt-search-relate .word.selected"),t=window.F.context("page").title;if(e.length){var a=[];s.each(e,function(e,t){a.push(s(t).find(".word-text").text())}),t=a.join(" ")}window.open("http://www.baidu.com/s?wd="+encodeURIComponent(t)+"&ie=utf-8&tn=SE_PCzhidaosszl_6wvraksp","_blank")}else s(this).hasClass("selected")?(s(this).removeClass("selected"),n.send({page:"question",area:"search-relate-word-cancel",action:"click",type:2014,isQ:!!window.isQualityView})):(s(this).addClass("selected"),n.send({page:"question",area:"search-relate-word-add",action:"click",type:2014,isQ:!!window.isQualityView}))})}var s=e("common:widget/js/util/tangram/tangram"),n=e("common:widget/js/util/log/log");a.exports.init=i});